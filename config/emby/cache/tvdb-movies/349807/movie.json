iel aus Verrat, Rache und Überlebenskampf.","language":"deu","IsPrimary":false}]},"contentRatings":[{"id":498,"name":"15","country":"kor","description":"Restricted to ages 15 and over, unless accompanied by an adult","contentType":"movie","order":0,"fullname":"15"}],"companies":{"studio":[],"network":[],"production":[],"distributor":[],"special_effects":[]},"production_countries":[{"id":284890,"country":"kor","name":"South Korea"}],"inspirations":["{id:41289,type:3,type_name:Book,url:https://www.goodreads.com/book/show/25542001}"],"spoken_languages":"[kor]","first_release":{"country":"kor","date":"2025-04-30"}}